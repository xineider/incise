
    <div class="card-panel white">
      <h4>Usuários</h4>
      <a href="/sistema/usuarios/cadastrar"  class="btn-floating btn-large waves-effect waves-light blue darken-3 right ajax-load tooltipped" data-position="left" data-delay="50" data-tooltip="Adicionar Usuario"><i class="fa fa-plus" aria-hidden="true"></i></a>
      <table class="center-all">
        <thead>
          <tr>
            <th>
              Empresa
            </th>
            <th>
              Login
            </th>
            <th>
              Email
            </th>
            <th>
              Ação
            </th>
          </tr>
        </thead>
        <tbody>
          <%
          if (typeof data != 'undefined' && data.length > 0) {
            for (var i = data.length - 1; i >= 0; i--) {
          %>
          <tr>
             <td>
              <% if(data[i].id_empresa==0) {%>
                Incise
              <% }else {%>
                <%= data[i].empresa %>
              <%}%>
            </td>
              <td>
              <%= data[i].login %>
            </td>
              <td>
              <%= data[i].email %>
            </td>
            <td>
              <a class="btn-floating btn waves-effect waves-light green ajax-load" href="/sistema/usuarios/editar/<%= data[i].id %>"><i class="fa fa-pencil" aria-hidden="true"></i></a>
              <a class="btn-floating btn waves-effect waves-light red modal-remover-mount" data-href="#modaldeletar" data-id="<%= data[i].id %>" data-texto="Você tem certeza que quer deletar '<%= data[i].nome_fantasia %>'? " data-to="/sistema/usuarios/desativar" data-back="/sistema/usuarios"><i class="fa fa-minus" aria-hidden="true"></i></a>
            </td>
          </tr>
          <%
            }
          } else {
          %>
          <tr>
            <td colspan="5" class="center-align">
              Não existem usuarios cadastradas
            </td>
          </tr>
          <%
            }
          %>
        </tbody>
      </table>
    </div>