  
    <div class="card-panel white">
      <br>
      <h4>Editar Usuario</h4>
      <form method="POST" action="" enctype="multipart/form-data">
        <%
        if (typeof data != 'undefined' && data.length > 0) {
          for (var i = data.length - 1; i >= 0; i--) {
        %>
        <div class="right-align">
          <a href="/sistema/usuarios/" class="btn-floating btn-large waves-effect waves-light blue darken-1 left ajax-load"><i class="fa fa-arrow-left" aria-hidden="true"></i></a>
          <button data-href="/sistema/usuarios/atualizar/<%= data[i].id %>" data-action="/sistema/usuarios/" class="btn-floating btn-large waves-effect waves-light green darken-1 ajax-submit">
            <i class="fa fa-floppy-o" aria-hidden="true"></i>
          </button>
        </div>
        <div class="row">
          <h5>Informações</h5>
          <input required="true" type="hidden" name="id" value="<%= data[i].id %>">

          <div class="input-field col s12 l3">
            <input required="true" class="validate" type="text" name="login" value="<%= data[i].login %>">
            <label for="login">Nome de Login</label>
          </div>

          <div class="input-field col s12 l3">
            <input required="true" class="validate" type="password" name="senha" required="true">
            <label for="senha">Senha</label>
          </div>

          <div class="input-field col s12 l6">
            <input required="true" class="validate" type="text" name="email" value="<%= data[i].email %>">
            <label for="email">Email</label>
          </div>
        <div class="right-align">
          <button data-href="/sistema/usuarios/atualizar/<%= data[i].id %>" data-action="/sistema/usuarios/" class="btn waves-effect waves-light green darken-1 ajax-submit">
            Salvar <i class="fa fa-floppy-o" aria-hidden="true"></i>
          </button>
        </div>
        <%
          }
        }
        %>
      </form>
    </div>
